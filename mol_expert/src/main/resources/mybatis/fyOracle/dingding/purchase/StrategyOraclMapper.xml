<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mol.expert.mapper.fyOracle.dingding.purchase.StrategyOraclMapper">
    <select id="getOraclId" resultType="java.lang.String" parameterType="java.lang.String">
        select
        pk_material
        from
        bd_material@ncdb
        where name =#{itemName}
    </select>
</mapper>