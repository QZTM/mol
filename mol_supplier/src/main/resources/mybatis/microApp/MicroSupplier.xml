<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mol.supplier.mapper.microApp.MicroSupplierMapper">

    <select id="getVersionByPkSupplier" parameterType="String" resultType="Integer">
        select version from bd_supplier where pk_supplier=#{pkSupplier}
    </select>

</mapper>